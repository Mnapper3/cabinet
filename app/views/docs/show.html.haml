.warpper_with_padding
    #doc_show
        %h1= @doc.title
        %p= @doc.content
            
            
        .buttons 
            =link_to "Fix Doc", edit_doc_path(@doc), class: "button"
            =link_to "Trash it", doc_path(@doc), method: :delete, date: {confirm: "Are You Sure?"}, class: "button"